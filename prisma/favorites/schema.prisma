datasource db {
  provider = "sqlite"
  url      = "file:../favoritesDatabase.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Favorites {
  id        String   @id @default(uuid())
  postId    String
  hearts    Int      @default(0)
  createdBy String
  createdAt DateTime @default(now())
}
